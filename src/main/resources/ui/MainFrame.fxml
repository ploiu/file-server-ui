<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<fx:root prefHeight="600.0" prefWidth="800.0" styleClass="frame" stylesheets="@styles.css"
         type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/17.0.2-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TextField fx:id="searchField" layoutX="14.0" layoutY="14.0" onKeyPressed="#searchBarKeyPressed"
                   prefHeight="24.0" prefWidth="333.0"/>
        <Button fx:id="searchButton" layoutX="347.0" layoutY="14.0" mnemonicParsing="false"
                onAction="#searchButtonClicked" styleClass="search-button" text="Search"/>
        <HBox fx:id="navigationBar" layoutX="14.0" layoutY="45.0" styleClass="nav-bar"/>
        <FlowPane fx:id="itemPane" layoutX="13.0" layoutY="64.0" prefHeight="528.0" prefWidth="777.0"/>
    </children>
</fx:root>
