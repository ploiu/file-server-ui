<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<fx:root xmlns:fx="http://javafx.com/fxml" type="HBox"
         xmlns="http://javafx.com/javafx" stylesheets="@SearchBar.css">
    <TextField fx:id="searchField"
               prefHeight="24.0" prefWidth="333.0" styleClass="search-bar" onKeyPressed="#searchBarKeyPressed"/>
    <Button fx:id="searchButton" mnemonicParsing="false"
            styleClass="search-button" text="Search" onAction="#searchButtonClicked"/>
</fx:root>
