<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.layout.Region?>
<fx:root stylesheets="@VideoPlayer.css" type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx/17.0.2-ea"
         xmlns:fx="http://javafx.com/fxml/1" styleClass="root">
    <MediaView fx:id="view"/>
    <StackPane fx:id="trackBox" styleClass="track-box" prefWidth="0">
        <Region fx:id="trackBackground" styleClass="track-background" prefWidth="${trackBox.prefWidth}"/>
        <Region fx:id="trackForeground" styleClass="track-foreground" prefWidth="0"/>
    </StackPane>
</fx:root>
